window.Feedback={open:function(){$("#feedback-dialog").dialog({closeOnEscape:!1,autoOpen:!1,height:410,width:450,modal:!0,buttons:[{text:AI.translate("send_btn_label"),click:function(){var a=$("#feedback_msg").val();AI.LoadingIndicator.on();var o=$.ajax({type:"POST",url:"/feedback",data:JSON.stringify({msg:a}),contentType:"application/json; charset=utf-8",dataType:"json"});o.done(function(){AI.Common.Notification.success(AI.translate("thank_you_for_feedback")),$("#feedback-dialog").dialog("close"),AI.LoadingIndicator.off()}),o.fail(function(a,o){AI.Common.Notification.error(AI.translate("error_sending_feedback")),$("#feedback-dialog").dialog("close"),AI.LoadingIndicator.off()})}},{text:AI.translate("cancel_btn_label"),click:function(){$(this).dialog("close")}}]}),$("#feedback-dialog").dialog("open"),$("#feedback_msg").val("")}};
